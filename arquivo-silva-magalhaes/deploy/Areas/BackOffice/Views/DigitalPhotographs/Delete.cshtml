@model DigitalPhotograph

@{
    ViewBag.Title = DigitalPhotographStrings.DeleteTitleFormat;
    ViewBag.ReturnTo = Url.Action("Details", "Specimens", new { id = Model.SpecimenId });
    ViewBag.AlertMsg = DigitalPhotographStrings.DeleteWarningMessage;
    Layout = "~/Areas/BackOffice/Views/Shared/_DeletePageLayout.cshtml";
}

<img src="@Url.Action("GetImage", "DigitalPhotographs", new { id = Model.Id, size = "large" })"
     class="center-block" />

<hr style="visibility: hidden" />
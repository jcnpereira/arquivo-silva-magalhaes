<!-- Modal -->
<div class="modal fade" id="afsm-deleteModalAlert" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">@LayoutStrings.Attention</h4>
            </div>
            <div class="modal-body">
                <span id="afsm-delete-modal-warning-message"></span>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">@LayoutStrings.No</button>
                <button type="button" class="btn btn-danger" id="afsm-doDelete">@LayoutStrings.Yes</button>
            </div>
        </div>
    </div>
</div>

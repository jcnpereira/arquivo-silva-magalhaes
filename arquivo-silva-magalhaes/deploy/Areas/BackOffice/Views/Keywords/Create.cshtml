@model KeywordTranslation
@{
    ViewBag.Title = KeywordStrings.CreateTitleFormat;
    
    Layout = Request.IsAjaxRequest() ? null :  "~/Areas/BackOffice/Views/Shared/_EditPageLayout.cshtml"; 
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.Partial("_KeywordFields")

        @Html.Partial("_SubmitBackPartial", LayoutStrings.Create)
    </div>
}



